<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hisp.dhis.relationship.RelationshipItem" table="relationshipitem">

        <cache usage="read-write" />

        <id name="id" column="relationshipitemid">
            <generator class="native" />
        </id>

        <property name="trackedEntityInstance" not-null="false" />
        <property name="programInstance" not-null="false" />
        <property name="programStageInstance" not-null="false" />

    </class>
</hibernate-mapping>
