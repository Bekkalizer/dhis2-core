<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
        >
<hibernate-mapping>
    <class name="org.hisp.dhis.reservedvalue.ReservedValue" table="reservedvalue">

        <composite-id>
            <key-property name="ownerObject" column="ownerobject">
                <type name="org.hibernate.type.EnumType">
                    <param name="enumClass">org.hisp.dhis.common.Objects</param>
                    <param name="useNamed">true</param>
                    <param name="type">12</param>
                </type>
            </key-property>
            <key-property name="ownerUid" />
            <key-property name="key" />
            <key-property name="value" />
        </composite-id>

        <property name="expires" not-null="true"/>

    </class>
</hibernate-mapping>